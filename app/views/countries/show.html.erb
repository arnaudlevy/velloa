<% content_for :title, "#{@country}" %>
<div class="country">
  <header>
    <h1><%= @country %></h1>
  </header>
  <div class="articles">
    <div class="row justify-content-between">
      <%= render @articles %>
    </div>
    <%= paginate @articles, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
